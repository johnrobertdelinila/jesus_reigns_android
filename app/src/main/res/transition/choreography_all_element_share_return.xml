<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:duration="375"
    android:interpolator="@android:interpolator/fast_out_slow_in"
    tools:targetApi="lollipop">
    <changeBounds />

    <!-- for activity transition -->
    <transition
        class="johnrobert.delinila.jesusreigns.transitions.FadeOutTransition"
        android:duration="75"
        android:interpolator="@android:interpolator/fast_out_linear_in"
        tools:targetApi="lollipop">
        <targets>
<!--            <target android:targetId="@id/all_element_share_no_share_text" />-->
            <!--<target android:targetId="@id/few_element_not_share_text" />-->
        </targets>
    </transition>

    <!-- for normal transition -->
    <fade
        android:duration="75"
        android:interpolator="@android:interpolator/fast_out_linear_in"
        tools:targetApi="lollipop">
        <targets>
<!--            <target android:targetId="@id/all_element_share_no_share_text" />-->
            <!--<target android:targetId="@id/few_element_not_share_text" />-->
        </targets>
    </fade>
</transitionSet>